{
  "schemaVersion": 1,
  "id": "crowd-control",
  "version": "${version}",
  "name": "Crowd Control",
  "description": "Allows viewers to interact with your Minecraft world",
  "authors": [
    "qixils"
  ],
  "contact": {
    "website": "https://github.com/qixils/minecraft-crowdcontrol",
    "repo": "https://github.com/qixils/minecraft-crowdcontrol.git"
  },
  "license": "MIT",
  "icon": "assets/crowd-control/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "dev.qixils.crowdcontrol.plugin.fabric.client.FabricPlatformClient"
    ],
    "main": [
      "dev.qixils.crowdcontrol.plugin.fabric.FabricCrowdControlPlugin"
    ],
    "cardinal-components": [
      "dev.qixils.crowdcontrol.plugin.fabric.interfaces.Components"
    ]
  },
  "mixins": [
    "crowd-control-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.19.0"
  },
  "custom": {
    "cardinal-components": [
      "crowd-control:viewer-mob",
      "crowd-control:original-display-name",
      "crowd-control:game-type-effect"
    ]
  }
}
